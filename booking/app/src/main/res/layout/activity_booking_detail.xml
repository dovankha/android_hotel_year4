<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.bookingdetail.booking_detail">

    <FrameLayout android:gravity="start"
        android:id="@+id/frameStackrectangle122"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        tools:ignore="MissingConstraints">

        <com.google.android.material.imageview.ShapeableImageView
            android:id="@+id/imageRectangle116"
            android:layout_gravity="start"
            android:layout_height="@dimen/_330pxh"
            android:layout_width="match_parent"
            android:scaleType="fitXY"
            android:background="@color/teal_400"
            app:shapeAppearanceOverlay="@style/rounfimageview"/>

        <LinearLayout
            android:id="@+id/linearColumnarrowleft"
            android:layout_gravity="top|start"
            android:layout_height="wrap_content"
            android:layout_marginHorizontal="@dimen/_36pxh"
            android:layout_marginVertical="@dimen/_36pxv"
            android:layout_width="wrap_content"
            android:orientation="vertical">
            <ImageButton
                android:background="@drawable/rectangle_bg_white_a700_radius_15"
                android:id="@+id/back"
                android:layout_gravity="start"
                android:layout_height="@dimen/_30pxh"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_width="@dimen/_30pxh"
                android:paddingHorizontal="@dimen/_9pxh"
                android:paddingVertical="@dimen/_9pxv"
                android:scaleType="fitCenter"
                android:src="@drawable/img_arrowleft_teal_400"/>
        </LinearLayout>

        <RelativeLayout
            android:layout_marginTop="@dimen/_30pxh"
            android:layout_width="match_parent"
            android:layout_gravity="center_vertical"
            android:layout_height="wrap_content">

            <LinearLayout

                android:gravity="center_vertical"
                android:id="@+id/process"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_120pxh"
                android:layout_width="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:ellipsize="end"
                    android:id="@+id/txtGroupNineteen"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:paddingBottom="@dimen/_3pxv"
                    android:paddingEnd="@dimen/_13pxh"
                    android:paddingStart="@dimen/_13pxh"
                    android:paddingTop="@dimen/_5pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_1"
                    style="@style/txtSolidRounded_1"/>

                <View android:background="@color/white_A700"
                    android:id="@+id/viewVectorOne"
                    android:layout_height="@dimen/_1pxv"
                    android:layout_marginBottom="@dimen/_14pxv"
                    android:layout_marginStart="@dimen/_5pxh"
                    android:layout_marginTop="@dimen/_15pxv"
                    android:layout_width="@dimen/_22pxh"/>

                <TextView android:ellipsize="end"
                    android:id="@+id/txtGroupEighteen"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_5pxh"
                    android:layout_width="wrap_content"
                    android:paddingBottom="@dimen/_3pxv"
                    android:paddingEnd="@dimen/_11pxh"
                    android:paddingStart="@dimen/_11pxh"
                    android:paddingTop="@dimen/_5pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_2"
                    style="@style/txtSolidRounded_2"/>

                <View android:background="@color/white_A700"
                    android:id="@+id/viewVectorTwo"
                    android:layout_height="@dimen/_1pxv"
                    android:layout_marginBottom="@dimen/_14pxv"
                    android:layout_marginStart="@dimen/_5pxh"
                    android:layout_marginTop="@dimen/_15pxv"
                    android:layout_width="@dimen/_22pxh"/>

                <TextView android:ellipsize="end"
                    android:id="@+id/txtGroupSeventeen"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_5pxh"
                    android:layout_width="wrap_content"
                    android:paddingBottom="@dimen/_3pxv"
                    android:paddingEnd="@dimen/_11pxh"
                    android:paddingStart="@dimen/_11pxh"
                    android:paddingTop="@dimen/_5pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_3"
                    style="@style/txtSolidRounded_2"/>
            </LinearLayout>

            <TextView
                android:layout_marginTop="@dimen/_10pxh"
                android:ellipsize="end"
                android:id="@+id/title"
                android:layout_gravity="center_vertical"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_120pxh"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="BookingSummary"
                android:layout_below="@+id/process"
                style="@style/txtPoppinsbold20_3" />

            <LinearLayout
                android:gravity="start"
                android:id="@+id/checkin_out"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_36pxh"
                android:layout_marginStart="@dimen/_36pxh"
                android:layout_marginTop="@dimen/_15pxv"
                android:layout_width="wrap_content"
                android:layout_below="@+id/title"
                android:orientation="vertical">

                <TextView
                    android:ellipsize="end"
                    android:id="@+id/txtNeermanaHotel"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="NeermanaHotel"
                    style="@style/txtPoppinsregular16_1"/>

                <LinearLayout android:gravity="start"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="wrap_content" android:orientation="horizontal">
                    <TextView android:ellipsize="end"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="CheckIn"
                        style="@style/txtPoppinsregular12_3"/>
                    <TextView
                        android:ellipsize="end"
                        android:id="@+id/txtCheckin"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_30pxh"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="Thu21 Feb 2023"
                        style="@style/txtPoppinsregular12_3"/>
                </LinearLayout>

                <LinearLayout android:gravity="start"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="wrap_content" android:orientation="horizontal">
                    <TextView android:ellipsize="end"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="CheckOut"
                        style="@style/txtPoppinsregular12_3"/>
                    <TextView
                        android:ellipsize="end"
                        android:id="@+id/txtCheckout"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_20pxh"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="Thu21 Feb 2023"
                        style="@style/txtPoppinsregular12_3"/>
                </LinearLayout>

            </LinearLayout>


            <LinearLayout
                android:gravity="start"
                android:id="@+id/PremiumDel"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_36pxh"
                android:layout_marginStart="@dimen/_36pxh"
                android:layout_marginTop="@dimen/_15pxv"
                android:layout_width="wrap_content"
                android:layout_below="@+id/checkin_out"
                android:orientation="vertical">

                <TextView
                    android:ellipsize="end"
                    android:id="@+id/title_room"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="(1) Premium Deluxe King Room"
                    style="@style/txtPoppinsregular16_1"/>

                <LinearLayout android:gravity="start"
                    android:id="@+id/linearRowuser"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="wrap_content"
                    android:orientation="horizontal">

                    <ImageView android:id="@+id/imageUser"
                        android:layout_height="@dimen/_10pxh"
                        android:layout_marginBottom="@dimen/_4pxv"
                        android:layout_marginTop="@dimen/_2pxv"
                        android:layout_width="@dimen/_8pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_user_10x8"
                        tools:ignore="ContentDescription"
                        tools:src="@drawable/img_user_10x8"/>

                    <TextView android:ellipsize="end"
                        android:id="@+id/txt1KingBed"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_6pxh"
                        android:layout_weight="0.87"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="KingBed"
                        style="@style/txtPoppinsregular12_3"
                        tools:text="@string/king_bed"/>
                </LinearLayout>

                <LinearLayout android:gravity="start"
                    android:id="@+id/linearRowvector"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:orientation="horizontal">

                    <ImageView android:id="@+id/imageVector"
                        android:layout_height="@dimen/_9pxh"
                        android:layout_marginBottom="@dimen/_3pxv"
                        android:layout_marginTop="@dimen/_4pxv"
                        android:layout_width="@dimen/_10pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_vector"
                        tools:ignore="ContentDescription"
                        tools:src="@drawable/img_vector"/>

                    <TextView android:ellipsize="end"
                        android:id="@+id/txtAdult"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_6pxh"
                        android:layout_weight="0.91"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="2 Adult(s) / Room"
                        style="@style/txtPoppinsregular12_3"/>
                </LinearLayout>



            </LinearLayout>



        </RelativeLayout>

    </FrameLayout>

    <androidx.core.widget.NestedScrollView
        android:id="@+id/body"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintTop_toBottomOf="@+id/frameStackrectangle122"
        app:layout_constraintBottom_toTopOf="@+id/linearColumnstartfrom"
        tools:ignore="MissingConstraints">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <TextView android:ellipsize="end"
                android:id="@+id/txtGuestDetails"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_10pxh"
                android:layout_marginEnd="@dimen/_36pxh"
                android:layout_marginStart="@dimen/_36pxh"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="GuestDetails"
                style="@style/txtPoppinsbold20"/>
            <EditText
                android:background="@drawable/rectangle_bg_gray_200_radius_20"
                android:ellipsize="end"
                android:hint="Your name"
                android:id="@+id/name"
                android:inputType="text"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_36pxh"
                android:layout_marginStart="@dimen/_36pxh"
                android:layout_marginTop="@dimen/_16pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_17pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_16pxh"
                android:paddingTop="@dimen/_18pxv"
                android:singleLine="true"
                android:text=""
                android:textColorHint="@color/bluegray_400"
                style="@style/etSolidRounded"
                tools:ignore="Autofill"/>
            <EditText
                android:background="@drawable/rectangle_bg_gray_200_radius_20"
                android:ellipsize="end"
                android:hint="email"
                android:id="@+id/email"
                android:inputType="textEmailAddress"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_36pxh"
                android:layout_marginStart="@dimen/_36pxh"
                android:layout_marginTop="@dimen/_16pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_18pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_16pxh"
                android:paddingTop="@dimen/_17pxv"
                android:singleLine="true"
                android:text=""
                android:textColorHint="@color/bluegray_400"
                style="@style/etSolidRounded" tools:ignore="Autofill"/>
            <EditText
                android:background="@drawable/rectangle_bg_gray_200_radius_20"
                android:ellipsize="end"
                android:hint="Day of birth"
                android:id="@+id/birth"
                android:inputType="text"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_36pxh"
                android:layout_marginStart="@dimen/_36pxh"
                android:layout_marginTop="@dimen/_16pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_18pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_16pxh"
                android:paddingTop="@dimen/_17pxv"
                android:singleLine="true"
                android:text=""
                android:focusable="false"
                android:textColorHint="@color/bluegray_400"
                style="@style/etSolidRounded" tools:ignore="Autofill"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtSpecialRequest"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_36pxh"
                android:layout_marginStart="@dimen/_36pxh"
                android:layout_marginTop="@dimen/_33pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="SpecialRequest"
                style="@style/txtPoppinsbold20"/>
            <EditText
                android:background="@drawable/rectangle_bg_gray_200_radius_20"
                android:ellipsize="end"
                android:hint="Your request"
                android:id="@+id/request"
                android:inputType="text"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_36pxh"
                android:layout_marginStart="@dimen/_36pxh"
                android:layout_marginTop="@dimen/_14pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_15pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_16pxh"
                android:paddingTop="@dimen/_20pxv"
                android:singleLine="true"
                android:textColorHint="@color/bluegray_400"
                style="@style/etSolidRounded"
                tools:ignore="Autofill"/>

            <TextView android:ellipsize="end"
                android:id="@+id/txtpromotion"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_36pxh"
                android:layout_marginStart="@dimen/_36pxh"
                android:layout_marginTop="@dimen/_33pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="Voucher"
                style="@style/txtPoppinsbold20"/>

            <LinearLayout
                android:layout_width="match_parent"
                android:orientation="horizontal"
                android:layout_height="wrap_content">
                <EditText
                    android:background="@drawable/rectangle_bg_gray_200_radius_20"
                    android:ellipsize="end"
                    android:hint="Add voucher"
                    android:id="@+id/promotion"
                    android:inputType="text"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_5pxh"
                    android:layout_marginStart="@dimen/_36pxh"
                    android:layout_marginTop="@dimen/_14pxv"
                    android:layout_width="@dimen/_200pxh"
                    android:paddingBottom="@dimen/_15pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_16pxh"
                    android:paddingTop="@dimen/_20pxv"
                    android:singleLine="true"
                    android:textColorHint="@color/bluegray_400"
                    style="@style/etSolidRounded"
                    tools:ignore="Autofill"/>

                <androidx.appcompat.widget.AppCompatButton
                    android:ellipsize="end"
                    android:gravity="center"
                    android:id="@+id/btnapply"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_marginEnd="@dimen/_36pxh"
                    android:layout_marginStart="@dimen/_5pxh"
                    android:layout_marginTop="@dimen/_16pxv"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_12pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_30pxh"
                    android:paddingTop="@dimen/_12pxv"
                    android:singleLine="true"
                    android:text="Apply"
                    android:textAllCaps="false"
                    style="@style/btnSolidRounded_1"/>
            </LinearLayout>

            <TextView android:ellipsize="end"
                android:id="@+id/notify"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_36pxh"
                android:layout_marginStart="@dimen/_36pxh"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="voucher"
                android:visibility="gone" />


        </LinearLayout>

    </androidx.core.widget.NestedScrollView>



    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearColumnstartfrom"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_5pxv"
        android:layout_width="match_parent"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/body"
        style="@style/rounfimageview">

        <LinearLayout android:id="@+id/linearRowstartfrom"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_36pxh"
            android:layout_marginStart="@dimen/_36pxh"
            android:layout_marginTop="@dimen/_10pxv"
            android:layout_width="match_parent"
            android:orientation="horizontal">

            <TextView android:ellipsize="end"
                android:id="@+id/txtStartFrom"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="StartFrom"
                style="@style/txtPoppinsbold12_2"/>

            <TextView android:ellipsize="end"
                android:id="@+id/txtPrice"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_210pxh"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="1000000"
                style="@style/txtPoppinsbold12"/>
        </LinearLayout>

        <androidx.appcompat.widget.AppCompatButton
            android:ellipsize="end"
            android:gravity="center"
            android:id="@+id/btnChooseRoom"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_20pxv"
            android:layout_marginEnd="@dimen/_36pxh"
            android:layout_marginStart="@dimen/_36pxh"
            android:layout_marginTop="@dimen/_16pxv"
            android:layout_width="match_parent"
            android:paddingBottom="@dimen/_15pxv"
            android:paddingEnd="@dimen/_30pxh"
            android:paddingStart="@dimen/_30pxh"
            android:paddingTop="@dimen/_15pxv"
            android:singleLine="true"
            android:text="Next"
            android:textAllCaps="false"
            style="@style/btnSolidRounded_1"/>
    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>